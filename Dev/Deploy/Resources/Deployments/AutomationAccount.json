{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "certificates_AzureClassicRunAsCertificate_base64Value": {
            "defaultValue": null,
            "type": "SecureString"
        },
        "certificates_AzureRunAsCertificate_base64Value": {
            "defaultValue": null,
            "type": "SecureString"
        },
        "automationAccounts_testtttt_name": {
            "defaultValue": "testtttt",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Automation/automationAccounts",
            "apiVersion": "2015-10-31",
            "name": "[variables('AAccount')]",
            "location": "westeurope",
            "properties": {}
        },
        {
            "type": "Microsoft.Automation/automationAccounts/certificates",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/AzureClassicRunAsCertificate')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "description": "This certificate is used to authenticate with Azure so that you can manage Azure classic resources from runbooks. For details on this certificate, or to manage it, go to this account’s Settings.  For example usage, see the tutorial runbook in this account.",
                "isExportable": false,
                "thumbprint": "29E31E882A8CF4BF676EE506458DFAD81ED7BC1B",
                "base64Value": "[parameters('certificates_AzureClassicRunAsCertificate_base64Value')]"
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/certificates",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/AzureRunAsCertificate')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "description": "This certificate is used to authenticate with the service principal that was automatically created for this account. For details on this service principal and certificate, or to recreate them, go to this account’s Settings. For example usage, see the tutorial runbook in this account.",
                "isExportable": true,
                "thumbprint": "2D2B036C2AB1A8BBEB69731AAB1859D880DFFCAD",
                "base64Value": "[parameters('certificates_AzureRunAsCertificate_base64Value')]"
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/connections",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/AzureClassicRunAsConnection')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "connectionType": {
                    "name": "AzureClassicCertificate"
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/connections",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/AzureRunAsConnection')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "connectionType": {
                    "name": "AzureServicePrincipal"
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/connectionTypes",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Azure')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "isGlobal": true,
                "fieldDefinitions": {
                    "AutomationCertificateName": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    },
                    "SubscriptionID": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/connectionTypes",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/AzureClassicCertificate')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "isGlobal": true,
                "fieldDefinitions": {
                    "SubscriptionName": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    },
                    "SubscriptionId": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    },
                    "CertificateAssetName": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/connectionTypes",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/AzureServicePrincipal')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "isGlobal": true,
                "fieldDefinitions": {
                    "ApplicationId": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    },
                    "TenantId": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    },
                    "CertificateThumbprint": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    },
                    "SubscriptionId": {
                        "isEncrypted": false,
                        "isOptional": false,
                        "type": "System.String"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Microsoft.PowerShell.Core')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Microsoft.PowerShell.Diagnostics')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Microsoft.PowerShell.Management')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Microsoft.PowerShell.Security')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Microsoft.PowerShell.Utility')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Microsoft.WSMan.Management')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('AAccount'), '/Orchestrator.AssetManagement.Cmdlets')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('AAccount'))]"
            ],
            "properties": {
                "contentLink": {}
            }
        }
    ]
}